#include<bits/stdc++.h>
using namespace std;
bool solve(int n){
    if(n==0 || n==1)return false;
    for(int i=2; i*i<=n; i++){
        if(n%i==0){
            return false;
        }
    }
    return true;
}

int main(){
    int t;
    cin>>t;
    int cnt=1;
    while(t--){
        string s;
        cin>>s;
        string ans;
        map<char,int>mp;
        for(int i=0; i<s.size(); i++){
            mp[s[i]]++;
        }
        for(auto it=mp.begin(); it!=mp.end(); it++){
            int p=it->second;
            if(solve(p)){
                ans+=it->first;
            }
        }
        if(ans.size()==0){
           cout<< "Case "<<cnt<<  ": "<<"empty"<<endl;
        }
        else{
            cout<< "Case "<<cnt<<  ": "<<ans<<endl;
        }

        cnt++;

    }

}
